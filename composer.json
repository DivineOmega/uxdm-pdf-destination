{
    "name": "divineomega/uxdm-pdf-destination",
    "description": "PDF destination for the UXDM data migrator",
    "type": "library",
    "authors": [
        {
            "name": "Jordan Hall",
            "email": "jordan@hall05.co.uk"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.0||^8.0",
        "fzaninotto/faker": "^1.6",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "require": {
        "php": "^7.1",
        "dompdf/dompdf": "^0.8.3",
        "divineomega/uxdm": "v3.0.0"
    },
    "autoload": {
        "psr-4": {
